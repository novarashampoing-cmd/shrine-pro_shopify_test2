        "custom_liquid_bwAHPH": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{%- assign fp = section.settings.product -%}\n{%- if fp and fp.has_only_default_variant == false -%}\n  {%- assign current_variant = fp.selected_or_first_available_variant -%}\n\n  <!-- ▼▼▼ Variant dropdown — HTML rendu côté Liquid ▼▼▼ -->\n  <div id=\"nk-dd-{{ section.id }}\" class=\"nk-dd-wrapper\" data-section-id=\"{{ section.id }}\" data-product-id=\"{{ fp.id }}\">\n    {%- for opt in fp.options_with_values -%}\n      {%- assign idx = forloop.index0 -%}\n      <div class=\"nk-dd-group\" data-opt-index=\"{{ idx }}\">\n        <label class=\"nk-dd-label\">{{ opt.name }}</label>\n        <div class=\"nk-dd\">\n          <select class=\"nk-dd-select\" aria-label=\"{{ opt.name }}\">\n            {%- assign selected_val = current_variant.options[idx] -%}\n            {%- for val in opt.values -%}\n              <option value=\"{{ val | escape }}\"{% if val == selected_val %} selected{% endif %}>{{ val }}</option>\n            {%- endfor -%}\n          </select>\n          <span class=\"nk-dd-caret\" aria-hidden=\"true\">▾</span>\n        </div>\n      </div>\n    {%- endfor -%}\n  </div>\n  <!-- ▲▲▲ /HTML ▲▲▲ -->\n\n  <style>\n    /* même design que précédemment */\n    .nk-dd-wrapper{ width:100%; margin:1rem 0; }\n    .nk-dd-group{ width:100%; margin-bottom:10px; }\n    .nk-dd-label{ display:block; margin:0 0 6px; font-size:14px; color:#222; }\n    .nk-dd{ position:relative; width:100%; background:#e2e6ea; border-radius:8px; overflow:hidden; height:44px; display:flex; align-items:center; }\n    .nk-dd-select{\n      appearance:none; -webkit-appearance:none; -moz-appearance:none;\n      border:none; background:transparent; width:100%; height:44px;\n      padding:0 40px 0 12px; font-size:16px; color:#000; cursor:pointer;\n    }\n    .nk-dd-select:focus{ outline:none; }\n    .nk-dd-caret{ position:absolute; right:10px; top:0; bottom:0; display:flex; align-items:center; pointer-events:none; font-size:16px; color:#000; }\n    .nk-dd-select:disabled{ opacity:.55; cursor:not-allowed; }\n    .nk-dd--invalid .nk-dd{ outline:2px solid #b00020; background:#fff; }\n  </style>\n\n  <script>\n  (function(){\n    var container = document.getElementById('nk-dd-{{ section.id }}');\n    if(!container) return;\n\n    var SECTION_ID = container.dataset.sectionId;\n    var scope = document.getElementById('shopify-section-' + SECTION_ID) || document;\n    var productId = container.dataset.productId;\n\n    // Variantes depuis Liquid (JSON sûr)\n    var variants = {{ fp.variants | json }};\n    // Nombre d'options\n    var optionCount = 0;\n    if (variants.some(function(v){return v.option1})) optionCount = 1;\n    if (variants.some(function(v){return v.option2})) optionCount = 2;\n    if (variants.some(function(v){return v.option3})) optionCount = 3;\n\n    var selects = Array.prototype.slice.call(container.querySelectorAll('.nk-dd-select'));\n\n    function selection(){\n      return selects.map(function(s){ return s.value; });\n    }\n    function findVariant(sel){\n      return variants.find(function(v){\n        var arr = [v.option1, v.option2, v.option3].slice(0, optionCount);\n        if (arr.length !== sel.length) return false;\n        for (var i=0;i<arr.length;i++){ if(String(arr[i]) !== String(sel[i])) return false; }\n        return true;\n      }) || null;\n    }\n\n    // ——— cibler tous les formulaires add-to-cart pertinents (section + sticky/drawer de CE produit)\n    var idSet = new Set(variants.map(function(v){ return String(v.id); }));\n    function targetForms(){\n      var forms = Array.prototype.slice.call(document.querySelectorAll('form[action*=\"/cart/add\"]'));\n      return forms.filter(function(f){\n        if (f.closest && f.closest('#shopify-section-' + SECTION_ID)) return true;\n        var fld = f.querySelector('[name=\"id\"]');\n        var val = fld && (fld.value || fld.getAttribute('value'));\n        return val ? idSet.has(String(val)) : false;\n      });\n    }\n\n    function setVariantOnForm(form, variantId){\n      var input = form.querySelector('input[name=\"id\"]');\n      var selEl = form.querySelector('select[name=\"id\"]');\n\n      if (selEl && Array.prototype.slice.call(selEl.options).some(function(o){return String(o.value)===String(variantId);})){\n        selEl.value = String(variantId);\n        selEl.dispatchEvent(new Event('input', {bubbles:true}));\n        selEl.dispatchEvent(new Event('change', {bubbles:true}));\n      } else {\n        if(!input){\n          input = document.createElement('input');\n          input.type = 'hidden';\n          input.name = 'id';\n          form.appendChild(input);\n        }\n        input.value = String(variantId);\n        input.dispatchEvent(new Event('input', {bubbles:true}));\n        input.dispatchEvent(new Event('change', {bubbles:true}));\n      }\n      Array.prototype.slice.call(form.querySelectorAll('[data-variant-id]')).forEach(function(el){\n        el.setAttribute('data-variant-id', String(variantId));\n      });\n    }\n\n    function ensureHiddenForBuyButtons(variantId){\n      var buyButtons = scope.querySelectorAll('buy-buttons[form], .buy-buttons[form]');\n      Array.prototype.forEach.call(buyButtons, function(bb){\n        var formId = bb.getAttribute('form');\n        if(!formId) return;\n        var hidden = container.querySelector('input[type=\"hidden\"][name=\"id\"][data-nk-form=\"'+formId+'\"]');\n        if(!hidden){\n          hidden = document.createElement('input');\n          hidden.type = 'hidden';\n          hidden.name = 'id';\n          hidden.setAttribute('data-nk-form', formId);\n          hidden.setAttribute('form', formId);\n          container.appendChild(hidden);\n        }\n        hidden.value = String(variantId);\n      });\n    }\n\n    function updateAvailability(){\n      var cur = selection();\n      selects.forEach(function(sel, idx){\n        Array.prototype.slice.call(sel.options).forEach(function(opt){\n          var test = cur.slice();\n          test[idx] = opt.value;\n          var possible = variants.some(function(v){\n            var arr = [v.option1, v.option2, v.option3].slice(0, optionCount);\n            for (var i=0;i<test.length;i++){\n              if (test[i] && String(arr[i]) !== String(test[i])) return false;\n            }\n            return v.available;\n          });\n          opt.disabled = !possible;\n        });\n        if(sel.selectedOptions[0] && sel.selectedOptions[0].disabled){\n          var firstEnabled = Array.prototype.slice.call(sel.options).find(function(o){return !o.disabled;});\n          if(firstEnabled) sel.value = firstEnabled.value;\n        }\n      });\n    }\n\n    function pushVariantEverywhere(variant){\n      var vid = variant.id;\n      targetForms().forEach(function(f){ setVariantOnForm(f, vid); });\n      ensureHiddenForBuyButtons(vid);\n      var detail = { variant: variant, productId: productId };\n      document.dispatchEvent(new CustomEvent('variant:change', {detail: detail, bubbles: true}));\n    }\n\n    function applySelection(){\n      var v = findVariant(selection());\n      if(v){\n        container.classList.remove('nk-dd--invalid');\n        pushVariantEverywhere(v);\n      } else {\n        container.classList.add('nk-dd--invalid');\n      }\n      updateAvailability();\n    }\n\n    // Init depuis form(s) existant(s) s’il y en a\n    (function initFromForms(){\n      var forms = targetForms();\n      var currentId = forms.map(function(f){\n        var fld = f.querySelector('[name=\"id\"]');\n        return fld && (fld.value || fld.getAttribute('value'));\n      }).find(Boolean);\n      var v = currentId ? variants.find(function(x){return String(x.id)===String(currentId);}) : null;\n\n      if(v){\n        var arr = [v.option1, v.option2, v.option3].slice(0, optionCount);\n        selects.forEach(function(sel, i){\n          var val = arr[i];\n          if(val && Array.prototype.slice.call(sel.options).some(function(o){return o.value===String(val);})){\n            sel.value = String(val);\n          }\n        });\n        pushVariantEverywhere(v);\n      } else {\n        updateAvailability();\n        var first = findVariant(selection()) || variants.find(function(x){return x.available;}) || variants[0];\n        if(first){\n          var arr2 = [first.option1, first.option2, first.option3].slice(0, optionCount);\n          selects.forEach(function(sel,i){ if(arr2[i]) sel.value = String(arr2[i]); });\n          pushVariantEverywhere(first);\n        }\n      }\n      updateAvailability();\n    })();\n\n    selects.forEach(function(sel){ sel.addEventListener('change', applySelection); });\n\n    var mo = new MutationObserver(function(){ applySelection(); });\n    mo.observe(document.body, {childList:true, subtree:true});\n  })();\n  </script>\n{%- endif -%}"
          }
        },
        "custom_liquid_eYzMQU": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<!-- 🔢 Sélecteur de quantité - même largeur que le bouton ATC (Impact) -->\n<div class=\"nk-qty-wrapper\">\n  <label class=\"nk-qty-label\" for=\"nk-qty-input\">Quantité</label>\n  <div class=\"nk-qty\">\n    <button type=\"button\" class=\"nk-qty-btn nk-qty-minus\" aria-label=\"Réduire\">−</button>\n    <input\n      id=\"nk-qty-input\"\n      type=\"number\"\n      name=\"quantity\"\n      value=\"1\"\n      min=\"1\"\n      class=\"nk-qty-input\"\n    >\n    <button type=\"button\" class=\"nk-qty-btn nk-qty-plus\" aria-label=\"Augmenter\">+</button>\n  </div>\n</div>\n\n<style>\n  .nk-qty-wrapper { margin: 1rem 0; width: 100%; }\n  .nk-qty-label { display:block; margin-bottom:6px; font-size:14px; color:#222; }\n  .nk-qty { display:grid; grid-template-columns:40px 1fr 40px; align-items:center; background:#e2e6ea; border-radius:8px; overflow:hidden; width:100%; }\n  .nk-qty-btn { height:44px; border:none; background:transparent; font-size:20px; font-weight:500; color:#000; cursor:pointer; display:flex; align-items:center; justify-content:center; }\n  .nk-qty-input { border:none; background:transparent; text-align:center; font-size:16px; height:44px; width:100%; appearance:textfield; }\n  .nk-qty-input::-webkit-outer-spin-button,.nk-qty-input::-webkit-inner-spin-button{ -webkit-appearance:none; margin:0; }\n</style>\n\n<script>\n(function(){\n  const root = document.currentScript.closest('.nk-qty-wrapper') || document;\n  const input = root.querySelector('#nk-qty-input');\n  const minus = root.querySelector('.nk-qty-minus');\n  const plus  = root.querySelector('.nk-qty-plus');\n\n  const MIN = () => Number(input.min) || 1;\n  const MAX = () => Number(input.max) || 9999;\n  const clamp = v => Math.max(MIN(), Math.min(v, MAX()));\n\n  function setVal(v){\n    input.value = clamp(Number(v)||MIN());\n    syncAllForms();\n    toggleMinus();\n  }\n  function toggleMinus(){ minus.disabled = (Number(input.value)||1) <= MIN(); }\n\n  minus.addEventListener('click', ()=> setVal((Number(input.value)||1)-1));\n  plus.addEventListener('click',  ()=> setVal((Number(input.value)||1)+1));\n  input.addEventListener('input', ()=> setVal(input.value));\n\n  // === SYNC → pousse la valeur vers TOUS les formulaires add-to-cart visibles (Impact + sticky bar)\n  function getATCForms(){\n    return Array.from(document.querySelectorAll('form[action*=\"/cart/add\"]'))\n      .filter(f => f.offsetParent !== null);\n  }\n  function ensureHiddenQty(form){\n    // Certains thèmes n'ont pas d'input quantity dans le form → on le crée caché\n    let q = form.querySelector('input[name=\"quantity\"]');\n    if(!q){\n      q = document.createElement('input');\n      q.type = 'hidden';\n      q.name = 'quantity';\n      form.appendChild(q);\n    }\n    return q;\n  }\n  function syncAllForms(){\n    const val = String(clamp(Number(input.value)||1));\n    getATCForms().forEach(form=>{\n      // 1) Mettez à jour tout input[name=\"quantity\"] existant (visible ou non)\n      form.querySelectorAll('input[name=\"quantity\"]').forEach(el => el.value = val);\n      // 2) S’il n’y en a pas, créez le hidden qty (certaines implémentations JS lisent directement dans le form)\n      ensureHiddenQty(form).value = val;\n    });\n  }\n\n  // === HOOK SUBMIT → avant soumission (ou click ATC), on force la quantité\n  function hookForm(form){\n    if(form.__nkHooked) return;\n    form.__nkHooked = true;\n\n    // Sur submit classique\n    form.addEventListener('submit', (e)=>{\n      syncAllForms(); // s'assure que la valeur part bien\n    }, true);\n\n    // Sur clic du bouton d'ajout (certains thèmes lisent la valeur à ce moment)\n    const addBtn = form.querySelector('[type=\"submit\"][name=\"add\"], button[name=\"add\"]');\n    if(addBtn){\n      addBtn.addEventListener('click', ()=> syncAllForms(), true);\n    }\n  }\n\n  // Associer aussi l'input à un form (attribut \"form\") pour compatibilité large\n  (function associateToNearestForm(){\n    const firstForm = getATCForms()[0];\n    if(firstForm){\n      if(!firstForm.id) firstForm.id = 'product-form-' + Math.random().toString(36).slice(2);\n      input.setAttribute('form', firstForm.id);\n    }\n  })();\n\n  // Initial sync + hook\n  syncAllForms();\n  getATCForms().forEach(hookForm);\n  toggleMinus();\n\n  // Variante change → on peut remettre à 1 si souhaité et resync\n  document.addEventListener('variant:change', ()=>{\n    setVal(1);\n  });\n\n  // Si le thème remplace dynamiquement le form (AJAX sections), on re-hook/sync\n  const mo = new MutationObserver(()=>{\n    getATCForms().forEach(hookForm);\n    syncAllForms();\n  });\n  mo.observe(document.body, {childList:true, subtree:true});\n})();\n</script>"
          }
        },
